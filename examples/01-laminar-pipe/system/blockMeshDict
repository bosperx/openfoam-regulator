(
    (0 0 0)
    (1 0 0)  // pts 1-3 are not used to build the geometry, but placed here define (x,y,z) directions
    (1 1 0)  // pt 0 to 1 = x, 1 to 2 = y, 0 to 4 = z
    (0 1 0)
    (10 0 0)
    (0 0.9990482216 -0.0436193874)
    (0 0.9990482216 0.0436193874)
    (10 0.9990482216 -0.0436193874)
    (10 0.9990482216 0.0436193874)
);

