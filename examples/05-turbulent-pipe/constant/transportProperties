nu              8e-07;
a               2.4e-06; // = k/(rho*Cp)

rho             rho     [1 -3 0 0 0 0 0 ] 1000;
Cp              Cp      [0 2 -2 -1 0 0 0 ] 4183;
k               k       [1 1 -3 -1 0 0 0 ] 10;

